{
  "name": "pre-compiled",
  "description": "Targeted precompile for ES6 bundles for Node environments: versions 0.10, 0.11, 0.12, 4, 5",
  "main": "index.js",
  "scripts": {
    "test": "DEBUG=compiled node index.js",
    "lint": "standard *.js features/*.js",
    "commit": "commit-wizard",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bahmutov/pre-compiled.git"
  },
  "keywords": [
    "ES6",
    "ES2015",
    "pre",
    "compile",
    "precompile",
    "pre-compile",
    "bundle",
    "rollup"
  ],
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bahmutov/pre-compiled/issues"
  },
  "homepage": "https://github.com/bahmutov/pre-compiled#readme",
  "config": {
    "pre-compiled": {
      "dir": "test-dist",
      "files": [
        "test/main.js",
        "test/bin-main.js"
      ],
      "features": [
        "StringMethods"
      ],
      "node": [
        "0.12",
        "4"
      ]
    },
    "release": {
      "verifyConditions": {
        "path": "condition-node-version",
        "node": "4.2.2",
        "verbose": true
      }
    },
    "pre-git": {
      "commit-msg": [
        "simple"
      ],
      "pre-commit": [
        "npm run lint",
        "npm test"
      ],
      "pre-push": [],
      "post-commit": [],
      "post-merge": []
    }
  },
  "devDependencies": {
    "condition-node-version": "1.2.0",
    "pre-git": "3.3.0",
    "semantic-release": "^4.3.5",
    "standard": "5.4.1"
  },
  "dependencies": {
    "check-more-types": "2.10.0",
    "compiled": "1.18.0",
    "lazy-ass": "1.3.0"
  }
}
